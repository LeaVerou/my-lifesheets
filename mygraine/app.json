{
	"app": {
		"name": "mygraine",
		"slug": "mygraine",
		"entry_name": "migraine",
		"entries_name": "migraines",
		"description": "migraine tracker",
		"private": false,
		"icon": "ðŸ¤•",
		"color": "#a00808",
		"storage_type": "",
		"storage": "",
		"custom_css": "",
		"charts_placement": ""
	},
	"temporal": {
		"times": "yes",
		"ranges": "yes"
	},
	"entry": [
		{
			"type": "expression",
			"scope": "entry",
			"flags": [
				"temporal",
				"deletable"
			],
			"settings": {
				"expression": "weekday(date, 'shortname')"
			},
			"name": "weekday"
		},
		{
			"type": "property",
			"scope": "entry",
			"flags": [
				"temporal"
			],
			"settings": {
				"type": "date",
				"default": "[$today]",
				"formatting": [
					"b"
				]
			},
			"name": "date"
		},
		{
			"type": "property",
			"scope": "entry",
			"flags": [
				"temporal"
			],
			"settings": {
				"prefix": " at ",
				"type": "time",
				"default": "[time($startup, 'hours')]",
				"format": "[time(time, 'minutes')]"
			},
			"name": "time"
		},
		{
			"type": "expression",
			"scope": "entry",
			"flags": [
				"temporal"
			],
			"settings": {
				"visible": "false",
				"expression": "datetime(date, time)"
			},
			"name": "date_time"
		},
		{
			"type": "property",
			"scope": "entry",
			"flags": [
				"temporal"
			],
			"settings": {
				"prefix": " to ",
				"type": "time",
				"default": "",
				"visible": "[time]",
				"format": "[time(time, 'minutes')]"
			},
			"name": "end_time"
		},
		{
			"type": "property",
			"scope": "entry",
			"flags": [
				"temporal"
			],
			"settings": {
				"type": "date",
				"prefix": " on ",
				"visible": "[if(time and end_time, if(end_date = date, 'edit', true), false)]",
				"default": "[if(end_time, if(end_time < time, date(date + 1 * days()), date))]",
				"min_date": "[date]"
			},
			"name": "end_date"
		},
		{
			"type": "expression",
			"scope": "entry",
			"flags": [
				"temporal"
			],
			"settings": {
				"visible": "false",
				"expression": "datetime(end_date, end_time)"
			},
			"name": "end_date_time"
		},
		{
			"type": "expression",
			"scope": "entry",
			"flags": [
				"temporal"
			],
			"settings": {
				"visible": "false",
				"expression": "end_date_time - date_time"
			},
			"name": "duration"
		},
		{
			"type": "expression",
			"scope": "entry",
			"flags": [
				"temporal",
				"deletable"
			],
			"settings": {
				"visible": "[end_date and (time and end_time)]",
				"prefix": "(",
				"suffix": ")",
				"expression": "duration(duration, 2)"
			},
			"name": "duration_readable"
		},
		{
			"type": "spacer",
			"scope": "entry",
			"settings": {}
		},
		{
			"type": "property",
			"scope": "entry",
			"settings": {
				"number_style": "bar",
				"bar_style": "discrete"
			},
			"name": "intensity"
		},
		{
			"type": "property",
			"scope": "entry",
			"settings": {
				"type": "options",
				"options": "left, right, both"
			},
			"name": "side"
		},
		{
			"type": "property",
			"scope": "entry",
			"settings": {
				"type": "text",
				"appearance": "line",
				"multiline": true
			},
			"name": "notes"
		}
	],
	"root": [
		{
			"type": "expression",
			"scope": "root",
			"settings": {
				"expression": "duration($now - last(end_date_time))",
				"suffix": " since last migraine",
				"appearance": "line"
			}
		},
		{
			"type": "action",
			"scope": "root",
			"settings": {
				"visible": "edit",
				"label": "âž• Start migraine",
				"target": "add(entry)",
				"target_preset": "new",
				"appearance": "stretch",
				"set_pairs": [],
				"filter_pairs": []
			}
		},
		{
			"type": "action",
			"scope": "root",
			"settings": {
				"target_preset": "last",
				"target": "last(entry)",
				"set_pairs": [
					{
						"name": "end_time",
						"value": "time($now)"
					}
				],
				"filter_pairs": [],
				"label": "End migraine",
				"appearance": "stretch"
			}
		}
	],
	"charts": [
		{
			"type": "chart",
			"scope": "charts",
			"settings": {
				"aggregate": "average",
				"aggregate_of": "intensity",
				"type": "bar"
			}
		},
		{
			"type": "chart",
			"scope": "charts",
			"settings": {
				"group_by": "weekday",
				"type": "bar"
			}
		},
		{
			"type": "chart",
			"scope": "charts",
			"settings": {
				"group_by": "side",
				"type": "pie"
			}
		}
	]
}